				<key>pkg_payload_path</key>
				<string>%RECIPE_CACHE_DIR%/unpack/datadog-agent-core.pkg/Payload</string>
				<key>purge_destination</key>
				<true/>
			</dict>
			<key>Processor</key>
			<string>PkgPayloadUnpacker</string>
		</dict>
		<dict>
			<key>Arguments</key>
			<dict>
				<key>input_plist_path</key>
